{
  "name": "sigecafe",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "npm run kill && npm run db && cross-env NODE_NO_WARNINGS=1 nuxt dev",
    "host": "npm run kill && npm run db && cross-env NODE_NO_WARNINGS=1 nuxt dev --host 0.0.0.0",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "install-playwright": "npx playwright install --with-deps chromium",
    "test": "node ./scripts/run-test.mjs",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:unit:watch": "vitest --config vitest.config.ts",
    "test:e2e": "playwright test --debug",
    "all": "npm install && npm run build && npm run preview",
    "env": "node scripts/env-create.mjs",
    "db": "npm run db:down && npm run db:up && npm run db:migrate && npm run db:seed",
    "db:up": "node scripts/db-create.mjs",
    "db:down": "node scripts/db-destroy.mjs",
    "db:seed": "node scripts/db-seed.mjs",
    "db:migrate": "prisma migrate dev --name init",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "kill": "node scripts/server-destroy.mjs"
  },
